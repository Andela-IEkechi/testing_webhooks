<table class="table table-bordered table-striped">
  <% @projects.each do |project| %>
  <tr class="clickable" data-href="<%= project_path(project) %>">
    <td><%= project.title %></td>
  </tr>
  <% end %>
  <% if @projects.count == 0 %>
  <tr>
    <td>There are no projects</td>
  </tr>
  <% end %>
</table>
